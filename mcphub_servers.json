{
  "mcpServers": {
    "allpepper-memory-bank": {
      "args": [
        "-y",
        "@allpepper/memory-bank-mcp"
      ],
      "autoApprove": [
        "memory_bank_read",
        "memory_bank_write",
        "memory_bank_update",
        "list_projects",
        "list_project_files"
      ],
      "command": "npx",
      "disabled": true,
      "env": {
        "MEMORY_BANK_ROOT": "~/.config/memory-bank"
      }
    },
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ],
      "command": "npx"
    },
    "duckduckgo": {
      "args": [
        "duckduckgo-mcp-server"
      ],
      "command": "uvx",
      "disabled": false
    },
    "filesystem": {
      "args": [
        "mcp-server-filesystem",
        "${CWD}"
      ],
      "command": "npx",
      "disabled": false
    },
    "git": {
      "args": [
        "mcp-server-git"
      ],
      "command": "uvx",
      "disabled": false
    },
    "memory": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "command": "npx",
      "disabled": false
    },
    "sequentialthinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx",
      "disabled": false
    },
    "time": {
      "args": [
        "mcp-server-time",
        "--local-timezone=Asia/Shanghai"
      ],
      "command": "uvx",
      "disabled": false
    }
  },
  "nativeMCPServers": {
    "mcphub": {
      "disabled": false
    },
    "neovim": {
      "disabled": false
    }
  }
}
