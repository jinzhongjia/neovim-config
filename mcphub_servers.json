{
  "mcpServers": {
    "git": {
      "args": ["mcp-server-git"],
      "command": "uvx"
    },
    "time": {
      "args": ["mcp-server-time", "--local-timezone=Asia/Shanghai"],
      "command": "uvx"
    },
    "fetch": {
      "env": {
        "DEBUG": "true",
        "SERVER_URL": null,
        "API_KEY": ""
      },
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    },
    "sequentialthinking": {
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "command": "npx"
    },
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp@latest"]
    }
  },
  "nativeMCPServers": []
}
