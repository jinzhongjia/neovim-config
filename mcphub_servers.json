{
  "mcpServers": {
    "git": {
      "args": ["mcp-server-git"],
      "command": "uvx"
    },
    "time": {
      "args": ["mcp-server-time", "--local-timezone=Asia/Shanghai"],
      "command": "uvx"
    },
    "filesystem": {
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/home/jin/"],
      "command": "npx"
    },
    "fetch": {
      "env": {
        "DEBUG": "true",
        "SERVER_URL": null,
        "API_KEY": ""
      },
      "command": "uvx",
      "args": ["mcp-server-fetch"]
    },
    "sequentialthinking": {
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "command": "npx"
    },
    "memory": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "env": {
        "MEMORY_FILE_PATH": "/home/jin/.mcphub/servers/memory/memory.json"
      }
    }
  },
  "nativeMCPServers": []
}

